struct Point
{
    x : u32;
    y : u32;
}

func main() s32
{
    v1 : u8;
    v2 : u16;
    v3 : u32;

    v4 : s8;
    v5 : s16;
    v6 : s32;

    v7 : byte;

    v8 : bool;

    point : Point;

    ptr : u32@;

    res1 : bool = v1 == 0 && v2 == 0 && v3 == 0;

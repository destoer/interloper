
import <graphics>

func main() s32
{
    constant X : u32 = 640;
    constant Y : u32 = 480;

    win := open_window("interloper",X,Y);

    while(!win.quit)
    {
        process_events(&win);

        draw_window(&win,win.screen);

        sleep_ms(16);
    }

    close_window(&win);

    return 0;
}
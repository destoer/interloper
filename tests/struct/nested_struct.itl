import "mem"


struct Player
{
    pos : Vec;
    vel : Vec;
}

struct Vec
{
    x : u32;
    y : u32;
}


func make_player() Player
{
    player : Player = 
    {
        {0,0},
        {4,5},
    };


    return player;
} 

func update(player : Player@)
{
    player.pos.x += player.vel.x;
    player.pos.y += player.vel.y;
}

func main() s32
{
    player : Player = make_player();

    update(&player);

    return player.pos.x * player.pos.y;
}

// expected 6

func main() s32
{
    str : c8[?] = "Hello there";

    set : u64 = 0;

    for(i : u32 = 0; i < str.len; i += 1)
    {
        str[i] = to_lower(str[i]);
        
        if(str[i] >= 'a' && str[i] <= 'z')
        {
            x : u8 = str[i] - 'a';
            set = set_bit(set,x);
        }
    }

    return hamming_weight(set);
}
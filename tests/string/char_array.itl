
// expected 6

func main() s32
{
    str : const string = "Hello there";

    set : u64 = 0;

    for(i : u32 = 0; i < str.len; i += 1)
    {
        c := to_lower(str[i]);
        
        if(c >= 'a' && c <= 'z')
        {
            x : u8 = c - 'a';
            set = set_bit(set,x);
        }
    }

    return hamming_weight(set);
}

// expected 6

func main() s32
{
    str : const string = "Hello there";

    set : u64 = 0;
    
    for c in str
    {
        l := to_lower(c);
        
        if(l >= 'a' && l <= 'z')
        {
            x : u8 = l - 'a';
            set = set_bit(set,x);
        }
    }

    return hamming_weight(set);
}
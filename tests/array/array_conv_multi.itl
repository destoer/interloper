func sum(arr : u32[][])
{
    ans : u32 = 0;

    for(i : u32 = 0; i < arr.len; i += 1)
    {
        for(j : u32 = 0; j < arr[i].len; j += 1)
        {
            ans += arr[i][j];
        }
    }

    return ans;
}

func main()
{
    arr : u32[?][?] = 
    {
        {1,2,3},
        {4,5,6},
    };

    return sum(arr);
}
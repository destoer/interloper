
// expected 16

func sum(arr: u32[]) u32
{
    ans : u32 = 0;

    for(i : u32 = 0; i < arr.len; i += 1)
    {
        ans += arr[i];
    }

    return ans;
}

func main() s32
{
    ans : u32 = 0;

    // fixed size but determined by initializer
    arr1 : u32[?] = {1,2,3};
    arr2 : u32[?] = {1,2,3,4};

    ans += sum(arr1);

    ans += sum(arr2);


    return ans;
}
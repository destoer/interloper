

// expected 720

// compute product of running total length, and totals of each array
func s32 main()
{
    
    u32[][] arr = 
    {
        {1}, // 1,1
        {1,2}, // 2,3
        {1,2,3,4},// 4, 10
        {5}, // 1, 5
        {1,2,3,4,5,7}, // 6 , 22
    };

    // 18, 40

    u32 len = 0;
    u32 total = 0;

    for(u32 i = 0; i < arr.len; i += 1)
    {
        len += arr[i].len;
        for(u32 j = 0; j < arr[i].len; j += 1)
        {
            total += arr[i][j];
        }
    }

    return len * total;
}
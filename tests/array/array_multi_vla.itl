// compute product of running total length, and totals of each array
// NOTE: dont worry about this yet, just get standard multidimensional arrays working
func s32 main()
{
    u32[1] arr1 = {1} // 1,1
    u32[2] arr2 = {1,2}, // 2,3
    u32[4] arr3 = {1,2,3,4},// 4, 10
    u32[1] arr4 = {5}, // 1, 5
    u32[6] arr5 = {1,2,3,4,5,7}, // 6 , 22

    u32[5][] arr = {arr1, arr2, arr3, arr4,arr5};

    // 18, 40

    u32 len = 0;
    u32 total = 0;

    for(u32 i = 0; i < arr.len; i += 1)
    {
        len += arr[i].len;
        for(u32 j = 0; j < arr[i].len; j += 1)
        {
            total += arr[i][j];
        }
    }

    return len * total;
}
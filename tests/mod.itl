

// https://projecteuler.net/problem=1

// multiple of 3 or 5
func bool is_multiple(s32 x)
{
    return ((x % 3) == 0) || ((x % 5) == 0);
}

// find sum of all natural numbers that are multiples of 3 or 5
// below 1000

func s32 main()
{
    s32 ans = 0;

    s32 x = 0;

    for(x < 1000)
    {
        // the branch past this is not present?
        if(is_multiple(x))
        {
            ans += x;
        }

        x += 1;
    }

    return ans;
}
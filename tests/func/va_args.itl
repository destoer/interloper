
// NOTE ... is sugar for Any[]
func sum_ints(boop : s32,args: ...) s32
{
    for(arg in args)
    {
        v := rtti::int_from_any(arg);

        boop += cast(s32,v);
    }

    return boop;
}


// expected 120
func main() s32
{
    x : s8 = 5;
    y : s16 = 7;
    z : s32 = 105;

    return sum_ints(3,x,y,z);
}
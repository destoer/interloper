enum scale  
{
    small,
    med,
    large,
}


func get_scale(v: u32) scale
{
    if v < 10
    {
        return scale::small;
    }

    else if v < 1000
    {
        return scale::med;
    }

    else
    {
        return scale::large;
    }
} 


func main() s32
{
    res1: bool =  get_scale(0) == scale::small && get_scale(50) == scale::med && get_scale(25252) == scale::large;

    //res2: bool = cast(u32,scale::small) == 0;

    //res3: bool = scale.size == 3;

    //res4: bool = cast(u32,scale::med) == 1;

    ans: bool = res1 /*&& res2 && res3 && res4*;

    if(ans)
    {
        return 0;
    }

    else 
    {
        return 1;
    }
}

// expected 450

// compute product of running total length, and totals of each array
func s32 main()
{
    u32[3][5] arr =
    {
        {0,1,2,3,4}, // 5 , 10
        {0,1,2,3,4}, // 5 , 10
        {0,1,2,3,4}, // 5 , 10
    };

    // 15, 30

    u32 len = 0;
    u32 sum = 0;

    for(u32 i = 0; i < arr.len; i++)
    {
        len += arr[i].len;

        for(u32 j = 0; i j < arr[i].len; j++)
        {
            sum += arr[i][j];
        }
    }

    return sum * len;
}
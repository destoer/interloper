
// expected 574

func s32 test(s32 x)
{
    if(x >= 5)
    {
        x = x - 1;

        if(x == 6)
        {
            x = x + 6;
        }

        else if(x == 7)
        {
            x = x + 10;
            return x;
        }

        else
        {
            x = x + 22;
        }

        x = x + 1;
    }

    else if(x == 0)
    {
        x = x + 100;
    }

    else
    {
        x = x + 99;
    }

    return x;
}


func s32 main()
{
    return test(0) + test(1) + test(2) + test(3) + 
        test(4) + test(5) +  test(6) + test(7);
}
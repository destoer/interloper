

// https://projecteuler.net/problem=1

// multiple of 3 or 5
func is_multiple(x: s32) bool
{
    return ((x % 3) == 0) || ((x % 5) == 0);
}

// find sum of all natural numbers that are multiples of 3 or 5
// below 1000

func main() s32
{
    ans : s32 = 0;

    for(x : s32 = 0; x < 1000; x += 1)
    {
        if(is_multiple(x))
        {
            ans += x;
        }
    }
    
    return ans;
}

func divide(v1: u32, v2: u32) (s32,bool)
{
    if(v2 != 0)
    {
        return v1 / v2,false;
    }

    return 0, true;
}

func numbers() (s32,s32)
{
    return 5, 10;
}


func basic() bool
{
    x : u32 = 5;
    err : bool = false;

    // basic 
    x, err = divide(1,0);

    basic : bool = false;

    if(err)
    {
        x, err = divide(30,2);

        return !err && x == 15;
    }

    return false;
}




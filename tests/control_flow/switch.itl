

func main() s32 
{
    buf : u32[?] = {0,1,5,2,0,1,6,9,10};

    ans : u32 = 0;

    for i : u32 = 0; i < buf.len; i += 1
    {
        switch(buf[i])
        {
            case 0:
            {
                ans += 5;
            }

            case 2:
            {
                ans *= 5;
            }

            case 5:
            {
                ans += 10;
            }

            default:
            {
                if(buf[i] & 1)
                {
                    ans += 35;
                }

                else
                {
                    ans += 25;
                }
            }
        }
    }

    return ans;
}
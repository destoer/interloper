
// expected 447
func main() s32 
{
    buf : s32[?] = {-1,1,5,2,12,0,6,9,10};

    ans : u32 = 0;

    for v in buf
    {
        switch(v)
        {
            case 1:
            {
                ans += 5;
            }

            case 2:
            {
                ans *= 5;
            }

            case 5:
            {
                ans += 10;
            }

            case 9:
            {
                ans -= 2;
            }
   
            default:
            {
                ans += 6;

                // 46
                if(is_set(cast(u32,v),0))
                {
                    ans += 35;
                }

                // 36
                else
                {
                    ans += 25;
                }

                ans += 5;
            }
        }
    }

    return cast(s32,ans == 447);
}
func u32 sum(u32[] arr)
{
    u32 ans = 0;

    for(u32 i = 0; i < arr.len; i += 1)
    {
        ans += arr[i];
    }

    return ans;
}

func s32 main()
{
    u32 ans = 0;
    u32[3] arr = {1,2,3};

    // array should get converted into a runtime sized array
    // when passed
    return sum(arr);
}
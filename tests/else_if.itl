
// expected 45077

func s32 test(s32 x)
{
    x = x + 5;

    // parens are optional
    if x < 0
    {
        x = 0x8000;
    }

    else if(x == 0)
    {
        x = 0x5000;
        return x;
    }

    else
    {
        return x + 0x1000;
    }

    return x + 6;
}

func s32 main()
{
    return test(0) + test(1) + test(-1) + test(-5000);
}
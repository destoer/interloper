
func main() s32
{
    for(i : u32 = 0; i < 10; i += 1)
    {
        print("count {}\n",i);
    }

    putchar('\n');

    print("{} is {} years old\n","john",20);

    v := 87;
    ptr := &v;

    print("{} : {b} : {x} : {}\n",v,v,v,ptr);

    s := -5;
    ptr_s := &s;
    print("{} : {b} : {x} : {}\n",s,s,s,ptr_s);

    print("{}\n",NULL);

    //invalid print
    //print("{} : {}\n","boop");

    return 0;
}